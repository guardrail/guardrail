<ruleset name="HTTPwatch.com">
	<target host="httpwatch.com" />
	<target host="blog.httpwatch.com" />
	<target host="www.httpwatch.com" />
	<rule from="^http://www\.httpwatch\.com/" to="https://www.httpwatch.com/"/>
	<rule from="^http://blog\.httpwatch\.com/" to="https://blog.httpwatch.com/"/>
	<rule from="^http://httpwatch\.com/" to="https://httpwatch.com/"/>
</ruleset>

