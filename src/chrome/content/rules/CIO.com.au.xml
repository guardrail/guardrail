<!-- idg does their best to make https tricky -->
<ruleset name="CIO.com.au">
	<target host="demo.idg.com.au" />
	<target host="cio.com.au" />
	<target host="www.cio.com.au" />
	<rule from="^http://demo\.idg\.com\.au/" to="https://demo.idg.com.au/"/>
	<rule from="^http://cio\.com\.au/" to="https://www.cio.com.au/"/>
	<rule from="^http://www\.cio\.com\.au/" to="https://www.cio.com.au/"/>
</ruleset>

