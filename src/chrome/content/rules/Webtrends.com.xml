<!-- tracking network, included on lots of sites. may carry banners.
If we are to download their content, atleast do it by https -->
<ruleset name="webtrends.com">
	<target host="m.webtrends.com" />
	<target host="developer.webtrends.com" />
	<target host="statse.webtrendslive.com" />
	<rule from="^http://m\.webtrends\.com/" to="https://m.webtrends.com/"/>
	<rule from="^http://developer\.webtrends\.com/" to="https://developer.webtrends.com/"/>
	<rule from="^http://statse\.webtrendslive\.com/" to="https://statse.webtrendslive.com/"/>
</ruleset>

