<ruleset name="SurveyMonkey">
  <target host="surveymonkey.com" />
  <target host="www.surveymonkey.com" />
  <target host="secure.surveymonkey.com" />

  <securecookie host="^(.+\.)?surveymonkey\.com$" name=".*"/>

  <rule from="^http://surveymonkey\.com/" to="https://www.surveymonkey.com/"/>
  <rule from="^http://(www|secure)\.surveymonkey\.com/" to="https://$1.surveymonkey.com/"/>
</ruleset>
