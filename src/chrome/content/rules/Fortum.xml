<!-- Fortum is nordic power company. -->
<ruleset name="Fortum.se">
	<target host="fortum.se" />
	<target host="www.fortum.se" />
	<target host="fortum.fi" />
	<target host="www.fortum.fi" />
	<rule from="^http://fortum\.se/" to="https://www.fortum.se/"/>
	<rule from="^http://www\.fortum\.se/" to="https://www.fortum.se/"/>
	<rule from="^http://fortum\.fi/" to="https://www.fortum.fi/"/>
	<rule from="^http://www\.fortum\.fi/" to="https://www.fortum.fi/"/>
</ruleset>

