<ruleset name="DuckDuckGo">
  <target host="duckduckgo.com" />
  <target host="*.duckduckgo.com" />
  <target host="duck.co" />
  
  <rule from="^http://duckduckgo\.com/" to="https://duckduckgo.com/"/>
  <rule from="^http://([^/:@]*)\.duckduckgo\.com/" to="https://$1.duckduckgo.com/"/>
  <rule from="^http://duck\.co/" to="https://duck.co/"/>
</ruleset>
